{
  "manifest_version": 2,
  "name": "UtubeTo",
  "description": "youtube url to google sheet attr",
  "version": "1.1",

  "background": {
    "page": "popup.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/youtube-icon.png"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
  "permissions": [
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "1004533533575-5pbgjp8c01cnro4v466ke55ig78vnioa.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets"]
  }


}